{
  "name": "atomic-algolia",
  "version": "0.3.19",
  "description": "An NPM package for running atomic updates to an Algolia index",
  "main": "lib/index.js",
  "bin": "lib/index.js",
  "files": [
    "lib/*"
  ],
  "scripts": {
    "prepublish": "npm run build",
    "prepush": "npm run build",
    "prepack": "npm run build",
    "build": "npm run babel",
    "start": "npm run babel:watch",
    "babel": "npx babel src -d lib",
    "babel:watch": "npx babel -- --watch",
    "clean": "npx rimraf lib",
    "test": "echo \"Error: no test specified\" && exit 0",
    "contributors:add": "npx all-contributors add",
    "contributors:generate": "npx all-contributors generate",
    "contributors:check": "npx all-contributors check"
  },
  "keywords": [
    "algolia",
    "node"
  ],
  "engines": {
    "node": ">=7.6.0"
  },
  "repository": "github:chrisdmacrae/atomic-algolia",
  "author": {
    "name": "Chris D. Macrae",
    "email": "hello@chrisdmacrae.com",
    "url": "https://chrisdmacrae.com"
  },
  "license": "ISC",
  "dependencies": {
    "@babel/cli": "^7.2.0",
    "@babel/core": "^7.2.0",
    "@babel/plugin-transform-runtime": "^7.2.0",
    "@babel/polyfill": "^7.0.0",
    "@babel/preset-env": "^7.2.0",
    "@babel/runtime": "^7.2.0",
    "algoliasearch": "^3.24.9",
    "chalk": "^2.3.0",
    "debug": "^4.1.0",
    "dotenv": "^5.0.0",
    "md5": "^2.2.1"
  },
  "devDependencies": {
    "all-contributors-cli": "^4.11.0",
    "eslint": "^4.19.0",
    "eslint-config-prettier": "^2.9.0",
    "husky": "^0.14.3",
    "prettier": "^1.11.1",
    "source-map-support": "^0.5.4",
    "stylelint": "^9.1.3",
    "stylelint-config-standard": "^18.2.0",
    "yarn": "^1.5.1"
  }
}
